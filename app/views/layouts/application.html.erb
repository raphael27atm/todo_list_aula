<!DOCTYPE html>
<html>
<head>
  <title>Aula</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |type, messages|%>
    <div class="alert-<%= type %>">
      <%= messages %>
    </div>
  <% end %>
  <%= yield %>
</body>
</html>
